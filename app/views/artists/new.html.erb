<hr color='red'>
<h1>CREATE ARTIST</h1>
<hr color='red'>
<% if flash[:errors] %>
  <% flash[:errors].each do |e| %>
  <%=e%><br>
  <% end %>
<% end %>

<%= form_for @artist do |f| %>
<%= f.label :"Name:"%>
<%= f.text_field :name %>

<%= f.label :"Age: "%>
<%= f.number_field :age%>

<%= f.label :"Title :"%>
<%= f.text_field :title %>

<%= f.submit %>
<% end %>